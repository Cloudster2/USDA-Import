{
	"name": "Trigger Testing Pipeline",
	"properties": {
		"activities": [
			{
				"name": "Trigger Testing API Import",
				"description": "importing from api on sched",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "RestSource",
						"httpRequestTimeout": "00:01:40",
						"requestInterval": "00.00:00:00.010",
						"requestMethod": "GET"
					},
					"sink": {
						"type": "SqlServerSink",
						"preCopyScript": "truncate table usda.TriggerTestingTable"
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "['reference_period_desc']"
								},
								"sink": {
									"name": "Period",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['year']"
								},
								"sink": {
									"name": "Year",
									"type": "Int32"
								}
							},
							{
								"source": {
									"path": "['statisticcat_desc']"
								},
								"sink": {
									"name": "Category",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['short_desc']"
								},
								"sink": {
									"name": "ItemConcat",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['group_desc']"
								},
								"sink": {
									"name": "GRP",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['week_ending']"
								},
								"sink": {
									"name": "WeekEndingString",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['region_desc']"
								},
								"sink": {
									"name": "Region",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['prodn_practice_desc']"
								},
								"sink": {
									"name": "ProdPract",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['state_ansi']"
								},
								"sink": {
									"name": "StateAnsi",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['domain_desc']"
								},
								"sink": {
									"name": "Domain",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['Value']"
								},
								"sink": {
									"name": "Value",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['freq_desc']"
								},
								"sink": {
									"name": "Frequency",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['commodity_desc']"
								},
								"sink": {
									"name": "Commodity",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['domaincat_desc']"
								},
								"sink": {
									"name": "DomainCat",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['util_practice_desc']"
								},
								"sink": {
									"name": "UtilPract",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['unit_desc']"
								},
								"sink": {
									"name": "Unit",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['state_name']"
								},
								"sink": {
									"name": "State",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['class_desc']"
								},
								"sink": {
									"name": "Class",
									"type": "String"
								}
							}
						],
						"collectionReference": "$['data']"
					}
				},
				"inputs": [
					{
						"referenceName": "NumberOfCows",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "testingtriggers",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}