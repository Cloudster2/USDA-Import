{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "AusSlaughter",
						"type": "DatasetReference"
					},
					"name": "Australia"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "EUGobalImport",
						"type": "DatasetReference"
					},
					"name": "australiasink"
				}
			],
			"transformations": [
				{
					"name": "derivedColumn1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          Column_1 as string,",
				"          Column_2 as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> Australia",
				"Australia derive(Column_1 = ERROR_FUNCTION(''),",
				"          Column_2 = ERROR_FUNCTION('')) ~> derivedColumn1",
				"derivedColumn1 sink(allowSchemaDrift: true,",
				"     validateSchema: true,",
				"     input(",
				"          EUID as integer,",
				"          Date as date,",
				"          DateString as string,",
				"          YearString as string,",
				"          Year as integer,",
				"          MonthString as string,",
				"          Month as integer,",
				"          ItemID as string,",
				"          ValueString as string,",
				"          Value as decimal(18,2)",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          DateString = Column_1,",
				"          ValueString = Column_2",
				"     )) ~> australiasink"
			]
		}
	}
}